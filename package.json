    "@rollup/plugin-node-resolve": "11.2.0",
    "@rollup/plugin-replace": "2.4.1",
    "@types/fs-extra": "9.0.8",
    "@types/jasmine": "3.6.7",
    "@types/node": "14.14.35",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "autoprefixer": "10.2.5",
    "chalk": "4.1.0",
    "clean-css": "5.1.2",
    "commander": "7.2.0",
    "convert-source-map": "1.7.0",
    "del": "6.0.0",
    "dts-bundle-generator": "5.8.0",
    "es6-plato": "1.2.3",
    "eslint": "7.22.0",
    "fs-extra": "9.1.0",
    "glob": "7.1.6",
    "html-minifier": "4.0.0",
