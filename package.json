{
  "name": "fieldwork",
  "description": "feasibility study",
  "version": "0.9.0",
  "author": "Shin Ogata",
  "bugs": {
    "url": "https://github.com/Shin-Ogata/fieldwork/issues"
  },
  "devDependencies": {
    "@cdp/tasks": "file:./packages/tool/tasks",
    "@rollup/plugin-alias": "2.2.0",
    "@types/jasmine": "3.5.0",
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "chalk": "3.0.0",
    "commander": "4.0.1",
    "convert-source-map": "1.7.0",
    "del": "5.1.0",
    "dts-bundle-generator": "3.3.1",
    "es6-plato": "1.2.3",
    "eslint": "6.7.2",
    "fs-extra": "8.1.0",
    "glob": "7.1.6",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "rollup": "1.27.13",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-multi-entry": "2.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "semver": "7.1.0",
    "terser": "4.4.3",
    "testem": "3.0.2",
    "typedoc": "0.15.5",
    "typescript": "3.7.3"
  },
  "homepage": "https://github.com/Shin-Ogata/fieldwork#readme",
  "keywords": [
    "C++",
    "TypeScript",
    "feasibility",
    "javascript"
  ],
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shin-Ogata/fieldwork.git"
  },
  "scripts": {
    "ci": "run-s packages:ci metrics",
    "clean:doc": "cdp-task clean -d",
    "clean:doc:a": "cdp-task clean -t ./docs/typedoc",
    "clean:doc:c": "cdp-task clean -t ./docs/reports/coverage",
    "clean:doc:m": "cdp-task clean -t ./docs/reports/metrics",
    "install": "cdp-task delegate install --preset -- --no-audit --no-bin-links --no-optional --no-update-notifier --loglevel=error --progress=false",
    "lint": "cdp-task delegate lint",
    "metrics": "run-p metrics:*",
    "metrics:file": "cdp-task metrics -r file -l ./config/lint/eslint.js -p lib",
    "metrics:module": "cdp-task metrics -r module -l ./config/lint/eslint.js -p lib",
    "packages:ci": "cdp-task delegate ci -l lib,framework",
    "packages:clean": "cdp-task delegate clean -l lib,framework",
    "packages:clear": "cdp-task delegate clear",
    "set-version": "cdp-task delegate set-version -l lib,framework -- ",
    "test": "npm run ci"
  }
}
