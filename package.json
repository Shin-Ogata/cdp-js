    "https-proxy-agent": "5.0.0",
    "jasmine-node": "3.0.0",
    "node-gyp": "7.1.2",
    "node-sass": "5.0.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "postcss": "8.2.8",
    "postcss-cli": "8.3.1",
    "rollup": "2.42.1",
    "semver": "7.3.4",
    "tar": "6.1.0",
